                    <FOREIGNID name="Javadoc" mc="Infrastructure.NoteType" uid="003800a8-0000-015f-0000-000000000000"/>
                  </LINK>
                </DEPENDENCIES>
              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="cdd28ce1-dc5c-4394-8ced-1e44951f302e"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[editorBounds = (Rectangle)editorComponent.getBounds(editorBounds);
diagram.remove(editorComponent);

// Update the area where the editor was
diagram.requestFocus();
DiagramUI ui = (DiagramUI)diagram.getUI();

ui.damageFigure(figure);
ui.repaintRegion(editorBounds);

// Discard the unused references
diagram = null;
editor = null;
figure = null;
editorComponent = null;
]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
