    //                bw.newLine();
    //                bw.write("Output.print(\"" + classname + ":" + m.group(2) + "\");");
    //                bw.newLine();
    //            }
    //            else {
    //                bw.write(newLine);
    //                bw.newLine();
    //            }
    //while
    bw.write(content);
    br.close();
    bw.close();
    System.out.println(source + ": Adding print methods successful!");
}
catch (Exception e) {
    System.out.println(e);
}
</body>
