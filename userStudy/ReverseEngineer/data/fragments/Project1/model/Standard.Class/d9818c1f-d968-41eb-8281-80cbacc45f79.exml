              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="a7039dd6-70d0-4606-9876-d5c3f9314a44"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[JFileChooser fileChooser = new JFileChooser();
fileChooser.setFileSelectionMode(JFileChooser.FILES_AND_DIRECTORIES);
fileChooser.setMultiSelectionEnabled(true);
fileChooser.setFileFilter(new FileFilter() {

    public boolean accept(File file) {
        String s = file.getName().toLowerCase();
        return file.isDirectory() || (file.isFile() && (
            s.endsWith(".xmi") || s.endsWith(".xml")
            || s.endsWith(".uml") // for AndroMDA
            || s.endsWith(".xmi.zip") || s.endsWith(".xml.zip")));
    }

    public String getDescription() {
        return "*.xmi *.xml *.xmi.zip *.xml.zip";
    }

});
]]></ATT>
