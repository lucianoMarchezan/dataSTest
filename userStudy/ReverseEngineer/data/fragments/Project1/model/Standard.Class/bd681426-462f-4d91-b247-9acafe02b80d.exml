                  <ATT name="Content"><![CDATA[Try to start editing a Figure
@param Diagram
@param Figure
@param EventObject]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
