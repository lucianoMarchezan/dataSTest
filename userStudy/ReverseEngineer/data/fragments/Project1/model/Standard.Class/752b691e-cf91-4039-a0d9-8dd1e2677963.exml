              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="03be6e89-7568-4ea4-9061-19bbb6ce4040"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[JRootPane rootPane = dialog.getRootPane(); 
rootPane.getInputMap(JComponent.WHEN_IN_FOCUSED_WINDOW)
        .put(KeyStroke.getKeyStroke(KeyEvent.VK_ESCAPE, 0),
                ACTION_KEY_ESCAPE);
// Add the action to the component
rootPane.getActionMap().put(ACTION_KEY_ESCAPE, new AbstractAction() {
    private static final long serialVersionUID = 0;

    public void actionPerformed(ActionEvent evt) {
        dialog.dispose();
    }
});
]]></ATT>
