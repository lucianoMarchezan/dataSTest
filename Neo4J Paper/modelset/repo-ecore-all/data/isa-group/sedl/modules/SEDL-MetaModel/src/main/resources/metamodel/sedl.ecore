                  eSuperTypes="#//es/us/isa/sedl/core/SedlEntity">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="A scientific hypothesis is a testable assertion. We distinguish three different types of hypotheses: descriptive, associational, and differential. "/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EReference" name="dependentVariable"
                    ordered="false" lowerBound="1" eType="#//es/us/isa/sedl/core/design/Outcome"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="DescriptiveHypothesis" eSuperTypes="#//es/us/isa/sedl/core/hypothesis/Hypothesis">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="A descriptive hypothesis just describes the properties of the concepts it refers to.&#xD;&#xA; In our context, a descriptive hypothesis is defined based on an assertion on the population.&#xD;&#xA;This assertion can be expressed in an informal way as a natural language description of &#xD;&#xA;the hypothesis of the experiment, or usen a formal assertion such as an statistical hypothesis.."/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EReference" name="assertion"
                    ordered="false" lowerBound="1" eType="#//es/us/isa/sedl/core/hypothesis/Assertion"
                    containment="true"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="RelationalHypothesis" abstract="true"
                  eSuperTypes="#//es/us/isa/sedl/core/hypothesis/Hypothesis">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="A relational hypothesis describes a relation between two variables. It is worth noting that the use of variables instead of concepts allows the &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;operationalization of the hypothesis (its situation in in the real world by specific rules of interpretation, that ultimately allow &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;its testing by reference to the observable phenomena).&#xD;&#xA;"/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EReference" name="independentVariables"
                    ordered="false" lowerBound="1" upperBound="-1" eType="#//es/us/isa/sedl/core/design/Factor"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="AssociationalHypothesis"
                  eSuperTypes="#//es/us/isa/sedl/core/hypothesis/RelationalHypothesis">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="An associational hypothesis states that the values of a set D of variables depends on the values of a different set of variables I according&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;to a specific relationship (usually expressed as a mathematical expression)."/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EReference" name="relation" ordered="false"
                    lowerBound="1" eType="#//es/us/isa/sedl/core/hypothesis/RelationSpecification"
                    containment="true"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="RelationSpecification"/>
              <eClassifiers xsi:type="ecore:EClass" name="DifferentialHypothesis"
