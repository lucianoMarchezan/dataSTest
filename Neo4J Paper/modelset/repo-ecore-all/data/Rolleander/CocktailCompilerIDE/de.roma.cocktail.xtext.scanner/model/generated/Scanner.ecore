        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rules" eType="#//Rule"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CodeBlock">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="wall" unique="false" upperBound="-1"
