              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="1386dadb-8d11-4cf5-8deb-7996b96d94c5"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Collection<UndoableAction> actions = new ArrayList<UndoableAction>();
Editor ce = Globals.curEditor();
List<Fig> figs = ce.getSelectionManager().getFigs();
for (Fig f : figs) {
    if (f instanceof PathContainer) {
        Object owner = f.getOwner();
        if (Model.getFacade().isAModelElement(owner)) {
            Object ns = Model.getFacade().getNamespace(owner);
            if (ns != null) {
                /* Only show the path item when there is 
                 * an owning namespace. */
                if (((PathContainer) f).isPathVisible()) {
                    actions.add(HIDE_PATH);
                    break;
                }
            }
        }
    }
}
for (Fig f : figs) {
    if (f instanceof PathContainer) {
        Object owner = f.getOwner();
        if (Model.getFacade().isAModelElement(owner)) {
            Object ns = Model.getFacade().getNamespace(owner);
            if (ns != null) {
                /* Only show the path item when there is 
                 * an owning namespace. */
                if (!((PathContainer) f).isPathVisible()) {
                    actions.add(SHOW_PATH);
                    break;
                }
            }
        }
    }
}
]]></ATT>
