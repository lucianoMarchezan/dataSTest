  <eClassifiers xsi:type="ecore:EClass" name="Thing" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A Thing's ID is usually an UUID representation. However, for things that have a distinct and universal meaning, e.g. Holy Quran Chapters, it's better to assign distinct human-readable IDs.&#xA;Human-readable IDs use the pattern {@code [a-z][a-z0-9_]+}. Note that UUID representations use the '-' character.&#xA;IDs need to be unique for all things."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="slug" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
