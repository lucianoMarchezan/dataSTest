              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="767d7f5c-9350-42da-9449-cb898f0ce6b5"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Object association = graphModel.connect(
        sourceFigNode.getOwner(), 
        destFigNode.getOwner(), 
        edgeType);

setNewEdge(association);

// Calling connect() will add the edge to the GraphModel and
// any LayerPersectives on that GraphModel will get a
// edgeAdded event and will add an appropriate FigEdge
// (determined by the GraphEdgeRenderer).

if (getNewEdge() != null) {
    getSourceFigNode().damage();
    destFigNode.damage();
    final Layer lay = editor.getLayerManager().getActiveLayer();
    final FigEdge fe = (FigEdge) lay.presentationFor(getNewEdge());
    _newItem.setLineColor(Color.black);
    fe.setFig(_newItem);
    fe.computeRoute();
    return fe;

} else {
    return null;
}
]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
