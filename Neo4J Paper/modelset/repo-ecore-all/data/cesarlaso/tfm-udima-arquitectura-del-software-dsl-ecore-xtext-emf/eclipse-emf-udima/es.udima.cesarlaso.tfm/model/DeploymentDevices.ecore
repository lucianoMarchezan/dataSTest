<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="deploymentdevices" nsURI="http://tfm.cesarlaso.udima.es/deploymentdevices"
    nsPrefix="es.udima.cesarlaso.tfm.deploymentdevices">
  <eClassifiers xsi:type="ecore:EClass" name="Device" abstract="true">
    <eOperations name="getDeviceNumber" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="wifiConfiguration" eType="#//WifiConfiguration"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OperatingSystemDevice" abstract="true"
      eSuperTypes="#//Device"/>
  <eClassifiers xsi:type="ecore:EClass" name="JavaDevice" abstract="true" eSuperTypes="#//OperatingSystemDevice"/>
  <eClassifiers xsi:type="ecore:EClass" name="Simulator" eSuperTypes="#//JavaDevice"/>
  <eClassifiers xsi:type="ecore:EClass" name="Raspberry" eSuperTypes="#//JavaDevice"/>
  <eClassifiers xsi:type="ecore:EClass" name="Beaglebone" eSuperTypes="#//JavaDevice"/>
  <eClassifiers xsi:type="ecore:EClass" name="ESP8266" eSuperTypes="#//Device"/>
  <eClassifiers xsi:type="ecore:EClass" name="WifiConfiguration">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ssid" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="password" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
