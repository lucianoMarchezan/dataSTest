      <eStructuralFeatures xsi:type="ecore:EAttribute" name="policy" ordered="false"
          eType="#//MONICA%20Typing%20Model/Policy"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="OrganizationBlock" eSuperTypes="#//MonitoringService">
