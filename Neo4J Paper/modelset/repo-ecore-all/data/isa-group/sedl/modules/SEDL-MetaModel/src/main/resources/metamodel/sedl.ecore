                <eStructuralFeatures xsi:type="ecore:EReference" name="constraints"
                    ordered="false" lowerBound="1" upperBound="-1" eType="#//es/us/isa/sedl/core/design/ProtocolConstraint"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="ExperimentalProtocolStep">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="An experimental protocol describes  the sequences of treatments  and measurements&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;to be performed on the different groups of the experiment. It is comprises of a se of steps, that can be&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;either measurements or treatments.&#xD;&#xA;A protocol step type makes reference to a set of groups where it is applied. If no group is specified, &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;It is assumed that the step is performed on all the groups of the experiment."/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EReference" name="group" ordered="false"
                    lowerBound="1" eType="#//es/us/isa/sedl/core/design/Group"/>
                <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" ordered="false"
                    lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String">
                  <eAnnotations source="http://www.eclipse.org/uml2/2.0.0/UML">
                    <details key="originalName" value="Id"/>
                  </eAnnotations>
                </eStructuralFeatures>
              </eClassifiers>
