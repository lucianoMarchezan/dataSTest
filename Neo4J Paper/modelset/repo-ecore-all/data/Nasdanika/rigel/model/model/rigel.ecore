    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Ссылка на работу"/>
      <details key="documentation_ru" value="???Ссылка на общий вид деятельности, определенный в другом месте."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="activity" eType="#//Activity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="By performing an activity participants produce outputs from inputs using resources."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Работа"/>
        <details key="documentation_ru" value="Выполняя работу исполнители производят исходящие (целевые) артефакты, используя входящие артефакты и ресурсы."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Artifact" eSuperTypes="#//PackageElement #//EngineeredElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Artifact is something output/produced by a source and consumed by a target as input. For example - source code, user guide, binary code. Artifacts are passed between activities over transitions."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Артефакт"/>
      <details key="documentation_ru" value="Артефакт - это что-то, что выводится / генерируется источником и используется целью в качестве входных данных. &#xA;&#xA;Например - исходный код, руководство пользователя, двоичный код. &#xA;&#xA;*Артефакт - это промежуточный результат, который передается от предшествующей операции (источника) к последующей операции (цели).*&#xA;&#xA;Артефакты передаются между действиями через переходы."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="consumers" upperBound="-1"
        eType="#//Target" eOpposite="#//Target/inputs">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Targets consuming this artifact as their input."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Потребители"/>
        <details key="documentation_ru" value="Работы или операции для которых данный аритефакт является входящим."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="producers" upperBound="-1"
        eType="#//Source" eOpposite="#//Source/outputs">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Sources emitting this artifact as their output."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Производители"/>
        <details key="documentation_ru" value="Работа или операция результатом которой является данный артефакт"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//Artifact" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Artifacts can be nested. E.g. a zip archive contains directories which contain files."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Вложенные артефакты"/>
        <details key="documentation_ru" value="Артефакты могут быть вложенными друг в друга. Например: zip, содержащий папки, содержащие файлы."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Resource" eSuperTypes="#//PackageElement #//EngineeredElement #//Capability">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Something leveraged by an actor to perform an activity. For example - a tool."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Ресурс"/>
      <details key="documentation_ru" value="Ресурс - объект, позволяющий исполнителю выполнять работу. Например, какой либо инструмент"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//Resource" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Resource can be nested. E.g. a virtual machine may host a web server."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Вложенные ресурсы"/>
        <details key="documentation_ru" value="Ресурсы могут быть вложенными, многокомпонентными. Например, виртуальная машина может включать в себя веб-сервер."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="artifacts" upperBound="-1"
        eType="#//Artifact" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Resources may host artifacts."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Артефакт"/>
        <details key="documentation_ru" value="Ресурсы могут включать в себя артефакты"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Transition" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Transition from its containing source element to a target element. May pass artifacts in both directions. E.g. a transition to &quot;Commit code&quot; may take &quot;Source code&quot; as input and return &quot;Commit ID&quot; as a result."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Переход (передача)"/>
      <details key="documentation_ru" value="Переход (передача) промежуточных результатов работ/операций от элемента-источника к целевому элементу. Артифакты могут передаваться в обоих направлениях. Например, переход к «Фиксирующему коду» может принимать «Исходный код» в качестве ввода и возвращать «Фиксированный код» в результате.&#xA; "/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//Target" eOpposite="#//Target/inboundTransitions">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Transition target."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Целевой элемент"/>
        <details key="documentation_ru" value="Работа/операция, являющаяся целью передачи артефакта, выполнение которой невозможно до выполнения работы-источника артефакта."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="inputs" upperBound="-1"
        eType="#//Artifact">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Artifacts passed by the source to the transition and delivered to the target."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Входящие артефакты"/>
        <details key="documentation_ru" value="Артефакты предаваемые от элемента-источника к целевому элементу через переход."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="results" upperBound="-1"
        eType="#//Artifact">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Artifacts returned by the target to be delivered to the source."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Результат передачи"/>
        <details key="documentation_ru" value="Артефакты возвращаемые целевым элементом элементу источнику."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Association" eSuperTypes="#//ModelElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="A generic relationship between model elements."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Ассоциация"/>
      <details key="documentation_ru" value="Отношения наследования между элементами модели"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//PackageElement">
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Цель"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Engineer" eSuperTypes="#//PackageElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Engineers own engineered elements and are assigned issues associated with these elements."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Инженер"/>
      <details key="documentation_ru" value="Специалист, владеющий каким-либо ресурсом, процессом или элементом процесса и получающий задачи, связанные с этим процессом или ресуром."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="owns" upperBound="-1" eType="#//EngineeredElement"
        eOpposite="#//EngineeredElement/owners">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Elements which this engineer owns. Ownership is propagated down to child elements recursively, unless these elements have an explicitly assigned owner. Also issues associated with owned elements and not having an engineer explicitly assigned to them are implicitly assigned to the element owner."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Хозяйство"/>
        <details key="documentation_ru" value="Совокупность элементов, которыми владеет инженер. &#xA;Владение распространяется до дочерних элементов рекурсивно, если только эти элементы не имеют явно назначенного владельца. Также проблемы, связанные с собственными элементами и отсутствием явно назначенного им инженера, неявно назначаются владельцу элемента."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignments" upperBound="-1"
        eType="#//Issue" eOpposite="#//Issue/assignedTo">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Engineer's assigned issues."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Задача"/>
        <details key="documentation_ru" value="Задачи, переданные инженеру на исполнение"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Issue" eSuperTypes="#//ModelElement #//Requirement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Something to possibly act on regarding the owning element - a problem/pain point, an improvement opportunity/enhancement.&#xA;&#xA;Example:&#xA;&#xA;* Containing activity - &quot;Initial setup of a software project&quot;, &#xA;* Size - 4.0 (hours) - copy an existing project and modify its sources.&#xA;* Issue - &quot;Create a code generator&quot; (enhancement),&#xA;    * Size - 40.0 (hours).&#xA;    * Benefit - 3.5 (hours).&#xA;    * Implementation - an activity providing a detailed explanation how code generator shall be implemented."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Задача"/>
      <details key="documentation_ru" value="Что-то, требующее выполнения какой-либо работы в отношении элемента-владельца - ошибка, проблема, инцедент, возможность усовершенствования."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="importance" eType="#//IssueImportance"
        defaultValueLiteral="Medium">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Issue importance."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="status" eType="#//IssueStatus"
        defaultValueLiteral="Open">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Issue status."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignedTo" eType="#//Engineer"
        eOpposite="#//Engineer/assignments">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Engineer this issue is assigned to. If this attribute is null the issue is assumed to be assigned to the owner of the containing element."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="An estimation of effort required to complete this issue in some units used consistently throughout the model - points, person hours, dollars."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="benefit" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="An estimation of reduction of the containing activity effort caused by completion of this issue. It can be used for cost/benefit analysis in order to prioritize issues."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//Issue" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Issues may be organized into a hierarchy."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="implementation" eType="#//Activity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Activity providing details about how to implement this issue.&#xA;&#xA;"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="IssueStatus">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Issue status."/>
    </eAnnotations>
    <eLiterals name="Open" literal="Open">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="An issue which hasn't been worked on."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="InProgress" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Work in progress."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Done" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Work completed."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Cancelled">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The issue is not going to be worked on for some reason."/>
      </eAnnotations>
    </eLiterals>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="IssueImportance">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Importance of the issue."/>
    </eAnnotations>
    <eLiterals name="Low">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Low importance."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Medium" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Medium importance."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="High" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="High importance."/>
      </eAnnotations>
    </eLiterals>
  </eClassifiers>
</ecore:EPackage>
