  <eSubpackages name="MONICA Design Model" nsURI="dfm.monica.mdm" nsPrefix="MDM">
    <eClassifiers xsi:type="ecore:EClass" name="UnitUnderMonitoring" eSuperTypes="#//MonitoringInfrastructure">
