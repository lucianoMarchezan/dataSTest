              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="cbd3ec2e-73da-46c3-a052-92f37cd14574"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[overwrite = canOverwrite;
Project p =  ProjectManager.getManager().getCurrentProject();
TargetManager tm = TargetManager.getInstance();
File saveDir = (directory != null) ? directory : getSaveDir(p);
if (saveDir == null) {
    /* The user cancelled! */
    return false;
}
boolean okSoFar = true;
ArgoDiagram activeDiagram = DiagramUtils.getActiveDiagram();
for (ArgoDiagram d : p.getDiagramList()) {
    tm.setTarget(d);
    okSoFar = trySaveDiagram(d, saveDir);
    if (!okSoFar) {
        break;
    }
}
tm.setTarget(activeDiagram);
]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
