                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Create a panel for editing the given set of UIManager font
properties.
@param String[]]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
                  <LINK relation="Model">
