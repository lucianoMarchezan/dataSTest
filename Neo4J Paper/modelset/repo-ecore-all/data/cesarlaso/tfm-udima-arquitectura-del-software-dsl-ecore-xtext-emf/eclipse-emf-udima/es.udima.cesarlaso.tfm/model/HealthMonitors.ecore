<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="healthmonitors" nsURI="http://tfm.cesarlaso.udima.es/healthmonitors"
    nsPrefix="es.udima.cesarlaso.tfm.healthmonitors">
  <eClassifiers xsi:type="ecore:EClass" name="HealthMonitor" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="HealthMonitorItem" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="time" lowerBound="1" eType="ecore:EClass Timers.ecore#//Timer"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="HealthMonitorData" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="adquisitionTimestamp" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Load" abstract="true" eSuperTypes="#//HealthMonitor"/>
  <eClassifiers xsi:type="ecore:EClass" name="CpuLoad" abstract="true" eSuperTypes="#//Load">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Carga de la CPU, especificado en porcentaje para cada una de las CPUs del sistema."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RamLoad" abstract="true" eSuperTypes="#//Load">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Carga actual de RAM del sistema, especificando el espacio total y el espacio libre "/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Temperature" abstract="true" eSuperTypes="#//HealthMonitor"/>
  <eClassifiers xsi:type="ecore:EClass" name="CpuTemperature" abstract="true" eSuperTypes="#//Temperature">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="s"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GpuTemperature" abstract="true" eSuperTypes="#//Temperature">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Temperatura de la GPU"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Storage" abstract="true" eSuperTypes="#//HealthMonitor"/>
  <eClassifiers xsi:type="ecore:EClass" name="Network" abstract="true" eSuperTypes="#//HealthMonitor"/>
  <eClassifiers xsi:type="ecore:EClass" name="StoragePartition">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="device" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mount" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="fileSystem" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="inodeFree" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="inodeTotal" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="spaceFree" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="spaceTotal" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StorageStats">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="device" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="reads" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="writes" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NetworkInterface">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rxBytes" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="txBytes" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rxPackets" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="txPackets" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CpuLoadItem" eSuperTypes="#//HealthMonitorItem #//CpuLoad"/>
  <eClassifiers xsi:type="ecore:EClass" name="RamLoadItem" eSuperTypes="#//HealthMonitorItem #//RamLoad"/>
  <eClassifiers xsi:type="ecore:EClass" name="CpuTemperatureItem" eSuperTypes="#//HealthMonitorItem #//CpuTemperature"/>
  <eClassifiers xsi:type="ecore:EClass" name="GpuTemperatureItem" eSuperTypes="#//HealthMonitorItem #//GpuTemperature"/>
  <eClassifiers xsi:type="ecore:EClass" name="StorageItem" eSuperTypes="#//HealthMonitorItem #//Storage"/>
  <eClassifiers xsi:type="ecore:EClass" name="NetworkItem" eSuperTypes="#//HealthMonitorItem #//Network"/>
  <eClassifiers xsi:type="ecore:EClass" name="CpuLoadData" eSuperTypes="#//HealthMonitorData #//CpuLoad">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="load" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RamLoadData" eSuperTypes="#//HealthMonitorData #//RamLoad">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="free" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CpuTemperatureData" eSuperTypes="#//HealthMonitorData #//CpuTemperature">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="temperature" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GpuTemperatureData" eSuperTypes="#//HealthMonitorData #//GpuTemperature">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="temperature" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StorageData" eSuperTypes="#//HealthMonitorData #//Storage">
    <eStructuralFeatures xsi:type="ecore:EReference" name="partitions" upperBound="-1"
        eType="#//StoragePartition" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stats" upperBound="-1"
        eType="#//StorageStats" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NetworkData" eSuperTypes="#//HealthMonitorData #//Network">
    <eStructuralFeatures xsi:type="ecore:EReference" name="interfaces" lowerBound="1"
        upperBound="-1" eType="#//NetworkInterface" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
