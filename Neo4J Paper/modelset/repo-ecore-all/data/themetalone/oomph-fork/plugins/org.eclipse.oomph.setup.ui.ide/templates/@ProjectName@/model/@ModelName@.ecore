    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="@PackageName@" nsURI="@NamespaceURI@" nsPrefix="@PackageName@">
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
