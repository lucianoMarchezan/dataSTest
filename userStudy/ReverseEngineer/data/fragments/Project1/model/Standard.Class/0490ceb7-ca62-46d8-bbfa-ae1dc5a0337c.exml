                <ID name="" mc="Infrastructure.Note" uid="41cbb557-069e-4b1b-aa64-5f98861b855e"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Test allAvailableFeatures and allAvailableContents including
inheritance from generalizations.]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
                  <LINK relation="Model">
