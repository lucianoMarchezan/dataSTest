      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Размер"/>
        <details key="documentation_ru" value="Оценка работы / операции в каких-либо единицах: трудо-часах, присестах, деньгах."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="progress" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
