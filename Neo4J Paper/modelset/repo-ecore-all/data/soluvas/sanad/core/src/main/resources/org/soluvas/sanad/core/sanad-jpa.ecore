        <details key="documentation" value="Slug which is based on initial name."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="canonicalSlug" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Canonical slug which is based on initial name."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="names" ordered="false"
        upperBound="-1" eType="#//Literal" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="authenticities" ordered="false"
