        <ownedOperation xmi:type="uml:Operation" xmi:id="_BQP6xbWpEeyVZOwzzImJqA" name="acceptOrder" visibility="public">
          <ownedComment xmi:type="uml:Comment" xmi:id="_BQP6xrWpEeyVZOwzzImJqA">
            <body>This method is called when the driver accepted an order.</body>
          </ownedComment>
          <ownedComment xmi:type="uml:Comment" xmi:id="_BQP6x7WpEeyVZOwzzImJqA">
            <body>if (taxiStruct.status == TaxiStruct.OFFLINE) {
    System.out.println("ComLinkC: Accept Order failed!");
}
else {
    System.out.println("ComLinkC: Order #" + orderNumber
                        + " accepted by driver.");
    System.out.println("ComLinkC: Estimated time until pickup:"
                        + estimateTime);
    taxiStruct.status = TaxiStruct.WAITING_FOR_CUSTOMER;
    taxiStruct.zone = zoneNumber;
}
</body>
          </ownedComment>
