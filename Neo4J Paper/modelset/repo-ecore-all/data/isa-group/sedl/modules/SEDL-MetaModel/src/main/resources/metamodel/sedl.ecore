              <eClassifiers xsi:type="ecore:EClass" name="FullySpecifiedExperimentalDesign"
                  eSuperTypes="#//es/us/isa/sedl/core/design/ExperimentalDesign">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="Custom experimental desgins must specify: the assignmet method used for grouping, the set of groups that are involved in the experiment, the &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;experimental protocol (i.e. the specific sequence of treatments and measurements to be performed on each group of experimental individuals), and&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;the set of analyses to be performed on the results of the experiments."/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EReference" name="assignmentMethod"
                    ordered="false" lowerBound="1" eType="#//es/us/isa/sedl/core/design/AssignmentMethod"
                    containment="true"/>
                <eStructuralFeatures xsi:type="ecore:EReference" name="groups" ordered="false"
                    upperBound="-1" eType="#//es/us/isa/sedl/core/design/Group" containment="true"/>
                <eStructuralFeatures xsi:type="ecore:EReference" name="experimentalProtocol"
                    ordered="false" lowerBound="1" eType="#//es/us/isa/sedl/core/design/ExperimentalProtocol"
                    containment="true"/>
                <eStructuralFeatures xsi:type="ecore:EReference" name="withinSubjectsVariables"
                    ordered="false" upperBound="-1" eType="#//es/us/isa/sedl/core/design/Variable"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="AssignmentMethod">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="&#x9;&#x9;&#x9;&#x9;The assignment method describes how the individuals in the population of the experiment are associated to the groups. The use of a random assignment &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;method is necessary for having an experiment, otherwise, we have a quasy-experiment. With quasi-experimental studies, it may not be possible to &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;demonstrate a causal link between the treatment condition and observed outcomes. On the other hand,  With random assignment, experimental subjects have the same &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;chance of being assigned to any group. As a result, all the groups will be statistically identical to the control group, on both controlled and&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;uncontrolled variables, at baseline (provided that the study has adequate sample size).&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;In SEDL assignment methods have a boolean attribute that describes if the assignment is random or not, and a description of the specic procedure used&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;for assignment (as a string in natural language).&#xD;&#xA;"/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EAttribute" name="description"
                    ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
                <eStructuralFeatures xsi:type="ecore:EAttribute" name="random" ordered="false"
                    lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"/>
                <eStructuralFeatures xsi:type="ecore:EReference" name="blockingVariables"
                    ordered="false" upperBound="-1" eType="#//es/us/isa/sedl/core/design/Variable"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="Group">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="A group is a set of experimental individuals that receive a sequence of treatments and &#xD;&#xA;&#x9;whose values for the dependent variables are measured in a specific sequence."/>
                </eAnnotations>
                <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" ordered="false"
                    lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
                <eStructuralFeatures xsi:type="ecore:EReference" name="sizing" ordered="false"
                    lowerBound="1" eType="#//es/us/isa/sedl/core/design/Sizing" containment="true"/>
                <eStructuralFeatures xsi:type="ecore:EReference" name="valuations"
                    ordered="false" upperBound="-1" eType="#//es/us/isa/sedl/core/design/VariableValuation"
                    containment="true"/>
              </eClassifiers>
              <eClassifiers xsi:type="ecore:EClass" name="Sizing">
                <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
                  <details key="documentation" value="A sizing describes the number of experimental individuals in the experiment. It can be a constant&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;size, but can also be an expression on the number of levels or value of some variables of&#xD;&#xA;&#x9;&#x9;&#x9;&#x9; the experiment."/>
                </eAnnotations>
              </eClassifiers>
