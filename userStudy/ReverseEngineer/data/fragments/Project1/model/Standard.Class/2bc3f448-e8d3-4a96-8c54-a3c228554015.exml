              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="d27dbb68-ac2b-4425-9b9e-fc5a0959b425"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[if (me.isConsumed()) {
    return;
}

// we initialize the coordinates.
x1 = me.getX();
x2 = me.getX();
y = me.getY();
lastX1 = x1 - 100;
lastY = y;
lastX2 = x1 + 100;
        
editor.damageAll();
me.consume();
start();
]]></ATT>
