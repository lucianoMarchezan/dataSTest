              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="abb502b2-e52f-4419-b585-df726249bef6"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[if (targetFollower == null) {
    targetFollower  = new ActionAddOperation();
    TargetManager.getInstance().addTargetListener(new TargetListener() {
        public void targetAdded(TargetEvent e) {
            setTarget();
        }
        public void targetRemoved(TargetEvent e) {
            setTarget();
        }

        public void targetSet(TargetEvent e) {
            setTarget();
        }
        private void setTarget() {
            targetFollower.setEnabled(targetFollower.shouldBeEnabled());
        }
    });
    targetFollower.setEnabled(targetFollower.shouldBeEnabled());
}
]]></ATT>
