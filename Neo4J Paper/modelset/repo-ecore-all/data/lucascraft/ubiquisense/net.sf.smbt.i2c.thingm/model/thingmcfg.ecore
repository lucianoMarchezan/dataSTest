<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="thingmcfg"
    nsURI="http://thingmcfg/1.0" nsPrefix="thingmcfg">
  <eClassifiers xsi:type="ecore:EClass" name="ThingMCfg">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="portAddr" eType="#//CommPortIdentifier"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="speed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="device" eType="#//ThingMDevice"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ThingMDevice">
    <eLiterals name="BLINK_M"/>
    <eLiterals name="MAX_M" value="1"/>
    <eLiterals name="MINI_M" value="2"/>
    <eLiterals name="CONTROL_M" value="3"/>
    <eLiterals name="REMOTE_M" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="CommPortIdentifier" instanceClassName="gnu.io.CommPortIdentifier"/>
</ecore:EPackage>
