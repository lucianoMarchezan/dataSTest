@param taxiNumber
@param operatorNumber</body>
          </ownedComment>
          <ownedComment xmi:type="uml:Comment" xmi:id="_BQQKobWpEeyVZOwzzImJqA">
            <body>if(taxiNumber &lt; 1) Output.printError("taxiNumber invalid.");
else{
    if(!voiceAlive &amp;&amp; voiceSend &amp;&amp; voiceConnecting){
        voiceAlive = true;
        voiceSend = true;
        voiceConnecting = false;
        central.confirmVoiceTM(taxiNumber,operatorNumber);
    }
    else
        System.out.println("confirmVoiceC: No voice communication started.");
}
</body>
