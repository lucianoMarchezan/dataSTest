              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="9b3db9a3-7676-44c4-b63b-436ff884b020"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[File dir = new File(argument);
if (!dir.exists() || !dir.isDirectory()) {
    LOG.log(Level.SEVERE, "The argument must be a path to an existing directory.");
    return false;
}
boolean result = true;
for (Project p : ProjectManager.getManager().getOpenProjects()) {
    TargetManager tm = TargetManager.getInstance();
    for (ArgoDiagram d : p.getDiagramList()) {
        tm.setTarget(d);
        if (!trySaveDiagram(d, dir)) {
            result = false;
        }
    }
}
]]></ATT>
