                <ID name="" mc="Infrastructure.Note" uid="08ca9ee1-4669-4729-8967-b3024f3c9435"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[// Check for a valid drag message
if(dragPoint == null) 
  return;

Point2D pt = e.getPoint();

double dx = pt.getX() - dragPoint.getX();
double dy = pt.getY() - dragPoint.getY();

paintOutline(dx, dy, false);

lastPoint = pt;
]]></ATT>
