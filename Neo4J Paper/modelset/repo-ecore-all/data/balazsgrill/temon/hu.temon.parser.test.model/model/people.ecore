<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="people" nsURI="http://people/1.0" nsPrefix="people">
  <eClassifiers xsi:type="ecore:EClass" name="People">
    <eStructuralFeatures xsi:type="ecore:EReference" name="people" upperBound="-1"
        eType="#//Human" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Human">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="father" eType="#//Man"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mother" eType="#//Woman"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Man" eSuperTypes="#//Human"/>
  <eClassifiers xsi:type="ecore:EClass" name="Woman" eSuperTypes="#//Human"/>
</ecore:EPackage>
