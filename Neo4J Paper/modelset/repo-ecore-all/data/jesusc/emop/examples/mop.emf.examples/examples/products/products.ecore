    <details key="name" value="BookStore"/>
    <details key="uri" value="http//test/bookstore"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="Data">
    <eStructuralFeatures xsi:type="ecore:EReference" name="products" upperBound="-1"
