  <eClassifiers xsi:type="ecore:EClass" name="Property" abstract="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Most properties exist because of {@link Claim}s, the rest are universal properties. One or more common Claims make up a Property.&#xA;&#xA;&lt;p>A property may be passivated when all of its claims are refuted. Universal properties (e.g. literals and transliterations of the Holy Quran) are permanent&#xA;and are not affected by Claims.&#xA;&#xA;&lt;p>Properties cannot be discussed. To &quot;discuss&quot; a Property, a user needs to either:&#xA;&#xA;&lt;ul>&#xA;&lt;li>create a Claim for a {@link Thing} for that property.&lt;/li>&#xA;&lt;li>create a Claim specifying the merits of an existing Claim.&lt;/li>&#xA;&lt;/ul>&#xA;"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Description of the property in AsciiDoc. It is derived from the {@link Claim} or {@link Testimony}'s description."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="descriptionHtml" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Description of the property in HTML format. It is derived from the {@link Claim} or {@link Testimony}'s {@code descriptionHtml}."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="claims" ordered="false"
        upperBound="-1" eType="#//Claim">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The set of Claims which proves this property."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
