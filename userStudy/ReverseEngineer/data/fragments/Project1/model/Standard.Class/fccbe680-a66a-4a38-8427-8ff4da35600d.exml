                <ID name="" mc="Infrastructure.Note" uid="f2fb4243-45e3-4db3-980f-01a21d0304fc"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Create an edge of the given type and connect it to the
given nodes.]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
                  <LINK relation="Model">
                    <FOREIGNID name="Javadoc" mc="Infrastructure.NoteType" uid="003800a8-0000-015f-0000-000000000000"/>
                  </LINK>
                </DEPENDENCIES>
              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="eb408102-8135-4228-992e-c4cc3fed4afb"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Object modelElement = graphModel.connect(
        fromElement.getOwner(),
        destFigNode.getOwner(),
        edgeType);

setNewEdge(modelElement);

// Calling connect() will add the edge to the GraphModel and
// any LayerPersectives on that GraphModel will get a
// edgeAdded event and will add an appropriate FigEdge
// (determined by the GraphEdgeRenderer).

if (getNewEdge() != null) {
    getSourceFigNode().damage();
    destFigNode.damage();
    Layer lay = editor.getLayerManager().getActiveLayer();
    FigEdge fe = (FigEdge) lay.presentationFor(getNewEdge());
    _newItem.setLineColor(Color.black);
    fe.setFig(_newItem);
    fe.setSourcePortFig(getStartPortFig());
    fe.setSourceFigNode(getSourceFigNode());
    fe.setDestPortFig(destFigNode);
    fe.setDestFigNode((FigNode) destFigNode);
    return fe;

} else {
    return null;
}
]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
