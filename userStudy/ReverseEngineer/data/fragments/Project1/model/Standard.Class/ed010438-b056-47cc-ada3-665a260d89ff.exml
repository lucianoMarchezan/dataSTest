                <ID name="" mc="Infrastructure.Note" uid="044684bd-0784-455e-9fb8-02261e8b780d"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Test the project setting for showing Visibility.]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
