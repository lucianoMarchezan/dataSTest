                <ID name="" mc="Infrastructure.Note" uid="578ec64e-7a6a-4774-a814-7624d8f5c943"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Copy the current selection from the Diagram to the Clipboard]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
                  <LINK relation="Model">
                    <FOREIGNID name="Javadoc" mc="Infrastructure.NoteType" uid="003800a8-0000-015f-0000-000000000000"/>
                  </LINK>
                </DEPENDENCIES>
              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="869ab82e-7dfe-491d-8fa4-74fa948ef745"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[if(!prepareSelection())
  return;

synchronized(figures) {

  // Clone the copied figures into a Vector for later processing
  clipboard.clear();
  for(int i=0; i<figures.length && figures[i] != null; i++) {
    
    Figure figure = figures[i];
    clipboard.add(figure);
    
  }
  
}
]]></ATT>
