              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="7431db2f-6486-4bfe-97be-89fe45a9e5cc"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[return false;]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
                  <LINK relation="Model">
                    <FOREIGNID name="JavaReturned" mc="Infrastructure.NoteType" uid="003800a8-0000-0190-0000-000000000000"/>
                  </LINK>
                </DEPENDENCIES>
              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="8f45b18f-6b5d-4175-b225-c0d6d8b8cf59"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[// Can't finish if our parent can't

if (!super.canFinish()) {
    return false;
}

// Can finish if it's step 0

if (getStep() == 0) {
    return true;
}

// Can finish if we're on step1 and have actually made a choice

if ((getStep() == 1) && (step1Choice != null)
        && (step1Choice.getSelectedIndex() != -1)) {
    return true;
}

// Otherwise we can't finish
]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
