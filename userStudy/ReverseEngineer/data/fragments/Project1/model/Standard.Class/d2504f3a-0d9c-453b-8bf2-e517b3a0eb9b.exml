                <ID name="" mc="Infrastructure.Note" uid="91fe23c4-a73f-4b27-af9c-84cd5e945f2c"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[JFileChooser chooser = getFileChooser(p);

String fn = Configuration.getString(
        SaveGraphicsManager.KEY_SAVEALL_GRAPHICS_PATH);
if (fn.length() > 0) {
    chooser.setSelectedFile(new File(fn));
}

int retval = chooser.showSaveDialog(ArgoFrame.getFrame());

if ( retval == JFileChooser.APPROVE_OPTION ) {
    File theFile = chooser.getSelectedFile();
    String path = theFile.getPath();
    Configuration.setString(
            SaveGraphicsManager.KEY_SAVEALL_GRAPHICS_PATH,
            path);
    return theFile;
}
]]></ATT>
