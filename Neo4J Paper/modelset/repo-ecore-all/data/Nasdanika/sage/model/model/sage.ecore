  <eClassifiers xsi:type="ecore:EClass" name="Organization">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Organization it the root of the Sage model. Organizations can be nested. Different products in organization's portfolio may be produced by different child organizations."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Организация"/>
      <details key="documentation_ru" value="Организация это корень модели Sage.  &#xA;Организации могут быть вложенными, то есть, иметь подразделения. &#xA;Различные продукты в портфеле организации могут разрабатываться разными подразделениями или командами  (дочерними организациями)."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="personas" upperBound="-1"
        eType="#//Persona" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Personas which represent the organization's target audiences. Typically shall be defined at the root organization, although child organizations may define internal personas."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Персона"/>
        <details key="documentation_ru" value="Заказчик, представитель целевой аудитории организации. Как правило, должны быть определены в корневой организации, хотя дочерние организации могут включать в себя представителей целевой аудитории."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="products" upperBound="-1"
        eType="#//AbstractProduct" containment="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Products manufactured by the organization. Products can be grouped into portfolios."/>
      </eAnnotations>
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Абстрактный продукт"/>
        <details key="documentation_ru" value="Продукт производимый организацией. Продукты должны быть объединены в портфели."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="strategies" upperBound="-1"
        eType="#//Strategy" containment="true">
      <eAnnotations source="urn:org.nasdanika">
        <details key="label_ru" value="Стратегия"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eGenericSuperTypes eClassifier="#//HierarchicalModelElement">
      <eTypeArguments eClassifier="#//Organization"/>
    </eGenericSuperTypes>
