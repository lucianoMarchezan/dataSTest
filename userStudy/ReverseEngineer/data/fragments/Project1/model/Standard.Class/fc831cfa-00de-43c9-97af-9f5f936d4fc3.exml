              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="e07db89e-98db-4aa4-9eed-64d9296a0371"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[URL url = null;
try {
    url = f.toURI().toURL();
} catch (MalformedURLException e) {
    LOG.log(Level.WARNING, "could not remove file ", e);
    return;
    }

List<URL> urls = new ArrayList<URL>();
for (URL u : getURLs()) {
    if (!url.equals(u)) {
        urls.add(u);
    }
}

// can't remove the last file
if (urls.size() == 0) {
    return;
    }

// can't remove from existing one so create new one.
URL[] urlArray = new URL[urls.size()];
urlArray = urls.toArray(urlArray);
instance = new ImportClassLoader(urlArray);
]]></ATT>
