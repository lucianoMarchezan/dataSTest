                <ID name="" mc="Infrastructure.Note" uid="4806d3b6-42dd-4973-9be0-62bc8259dd6e"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[int expectedModifiers = 
    (Modifier.PUBLIC | Modifier.STATIC | Modifier.FINAL);

Field[] fields = KeyEvent.class.getDeclaredFields();
for (int i = 0; i < fields.length; i++) {
    try {
        if (fields[i].getModifiers() == expectedModifiers
                && fields[i].getType() == Integer.TYPE
                && fields[i].getName().startsWith("VK_")
                && fields[i].getInt(KeyEvent.class) == keyCode) {

            return fields[i].getName().substring(3);
        }
    } catch (IllegalAccessException e) {

    }
}
]]></ATT>
