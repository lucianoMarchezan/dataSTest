<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="checkcfg" nsURI="http://www.avaloq.com/tools/ddk/checkcfg/CheckCfg"
    nsPrefix="checkcfg">
  <eClassifiers xsi:type="ecore:EClass" name="CheckConfiguration" eSuperTypes="#//ConfigurableSection">
    <eOperations name="getConfigurableSections" upperBound="-1" eType="#//ConfigurableSection"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="languageValidatorConfigurations"
        upperBound="-1" eType="#//ConfiguredLanguageValidator" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="legacyCatalogConfigurations"
        upperBound="-1" eType="#//ConfiguredCatalog" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="properties" upperBound="-1"
        eType="ecore:EClass platform:/plugin/com.avaloq.tools.ddk.check.core/metamodel/com/avaloq/tools/ddk/check/Check.ecore#//FormalParameter"
        volatile="true" transient="true" derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConfiguredLanguageValidator" eSuperTypes="#//ConfigurableSection">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="language" eType="ecore:EDataType platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="catalogConfigurations"
        upperBound="-1" eType="#//ConfiguredCatalog" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConfiguredCatalog" eSuperTypes="#//ConfigurableSection">
    <eStructuralFeatures xsi:type="ecore:EReference" name="catalog" eType="ecore:EClass platform:/plugin/com.avaloq.tools.ddk.check.core/metamodel/com/avaloq/tools/ddk/check/Check.ecore#//CheckCatalog"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="checkConfigurations" upperBound="-1"
        eType="#//ConfiguredCheck" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConfiguredCheck" eSuperTypes="#//ConfigurableSection">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="severity" eType="#//SeverityKind"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="check" eType="ecore:EClass platform:/plugin/com.avaloq.tools.ddk.check.core/metamodel/com/avaloq/tools/ddk/check/Check.ecore#//Check"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConfiguredParameter">
    <eStructuralFeatures xsi:type="ecore:EReference" name="parameter" eType="ecore:EClass platform:/plugin/com.avaloq.tools.ddk.check.core/metamodel/com/avaloq/tools/ddk/check/Check.ecore#//FormalParameter"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="newValue" eType="ecore:EClass platform:/plugin/org.eclipse.xtext.xbase/model/Xbase.ecore#//XExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SeverityKind">
    <eLiterals name="default" literal="default"/>
    <eLiterals name="error" value="1" literal="error"/>
    <eLiterals name="warning" value="2" literal="warning"/>
    <eLiterals name="info" value="3" literal="info"/>
    <eLiterals name="ignore" value="4" literal="ignore"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConfigurableSection">
    <eStructuralFeatures xsi:type="ecore:EReference" name="parameterConfigurations"
        upperBound="-1" eType="#//ConfiguredParameter" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
