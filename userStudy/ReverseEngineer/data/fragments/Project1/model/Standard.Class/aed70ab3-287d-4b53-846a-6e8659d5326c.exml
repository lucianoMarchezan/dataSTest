                  </LINK>
                </DEPENDENCIES>
              </OBJECT>
              <OBJECT>
                <ID name="" mc="Infrastructure.Note" uid="62eeeb6d-2cf5-48a8-aed5-6d64828fa86a"/>
                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[Editor ed = Globals.curEditor();
if (ed == null) {
    return;
}

if ((zoomFactor == 0)
        || ((ed.getScale() * zoomFactor 
            > ZoomSliderButton.MINIMUM_ZOOM / 100.0) 
        && ed.getScale() * zoomFactor 
            < ZoomSliderButton.MAXIMUM_ZOOM / 100.0)) {
    super.actionPerformed(arg0);
}
]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
