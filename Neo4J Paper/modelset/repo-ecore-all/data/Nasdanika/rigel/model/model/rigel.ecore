  <eClassifiers xsi:type="ecore:EClass" name="Activity" eSuperTypes="#//Flow #//Source #//Target">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="By performing an activity participants produce outputs from inputs using resources."/>
    </eAnnotations>
    <eAnnotations source="urn:org.nasdanika">
      <details key="label_ru" value="Работа"/>
      <details key="documentation_ru" value="Работа - это то, что необходимо для получения желаемого результата. &#xA;&#xA;Выполняя действие исполнитель получает входящие артефакты, использует ресурсы и создаёт исходящие артефакты, являющиеся промежуточными или конечными результатами процесса.&#xA;&#xA;Варианты перевода:&#xA;&#xA;&#xA;* Действие / Элементарное действие&#xA;* Процесс/Операция&#xA;* Работа / Операция "/>
    </eAnnotations>
