<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="properties" nsURI="http://www.novosoft.org/task/base/properties"
    nsPrefix="properties">
  <eClassifiers xsi:type="ecore:EEnum" name="OperationType">
    <eLiterals name="otBackupFull"/>
    <eLiterals name="otBackupIncremental" value="1"/>
    <eLiterals name="otBackupDifferential" value="2"/>
    <eLiterals name="otRestoreFull" value="3" literal=""/>
    <eLiterals name="otRestoreIncremental" value="4"/>
    <eLiterals name="otSynchronize" value="5"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ActionType">
    <eLiterals name="caRunProgram"/>
    <eLiterals name="caSendEmail" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ActionRunTime">
    <eLiterals name="caOnOperationStart"/>
    <eLiterals name="caOnOperationFinish" value="1"/>
    <eLiterals name="caOnOperationFailure" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CopyMode">
    <eLiterals name="cmSimple"/>
    <eLiterals name="cmCleanDestination" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CopyDirection">
    <eLiterals name="Direct"/>
    <eLiterals name="Reverse" value="1"/>
    <eLiterals name="Bidirectional" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="StopCondition">
    <eLiterals name="scOnCriticalError" literal=""/>
    <eLiterals name="scOnAnyError" value="1"/>
    <eLiterals name="scIgnoreAllErrors" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="DiffCondition">
    <eLiterals name="dcNewer"/>
    <eLiterals name="dcOlder" value="1"/>
    <eLiterals name="dcSmaller" value="2"/>
    <eLiterals name="dcLarger" value="3"/>
    <eLiterals name="dcNever" value="4" literal=""/>
    <eLiterals name="dcAlways" value="5"/>
    <eLiterals name="dcChanged" value="6"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ScheduleType">
    <eLiterals name="stOneShot"/>
    <eLiterals name="stEveryday" value="1"/>
    <eLiterals name="stWeekDays" value="2"/>
    <eLiterals name="stMonthDays" value="3"/>
    <eLiterals name="stPeriod" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ErrorID">
    <eLiterals name="SUCCESS"/>
    <eLiterals name="FAILED" value="1"/>
    <eLiterals name="CRITICAL" value="2" literal="CRITICAL"/>
    <eLiterals name="PATH_NOT_FOUND" value="3"/>
    <eLiterals name="ACCESS_DENIED" value="4"/>
    <eLiterals name="HANDLE_EOF" value="5"/>
    <eLiterals name="NOT_SUPPORTED" value="6"/>
    <eLiterals name="DISK_FULL" value="7"/>
    <eLiterals name="SESSION_LIMIT" value="8" literal="SESSION_LIMIT"/>
    <eLiterals name="STOPPED" value="9"/>
  </eClassifiers>
</ecore:EPackage>
