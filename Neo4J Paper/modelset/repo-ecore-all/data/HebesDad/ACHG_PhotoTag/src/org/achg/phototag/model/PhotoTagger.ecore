<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    name="PhotoTagModel"
    nsURI="http://org/achg/phototag"
    nsPrefix="org.achg.phototag">
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Root">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="XmlHome"
        lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral=""/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="Folders"
        upperBound="-1"
        eType="#//Folder"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="TagCategories"
        upperBound="-1"
        eType="#//TagCategory"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="Values"
        upperBound="-1"
        eType="#//TagValue"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="ImageCount"
        lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Folder">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="Folders"
        upperBound="-1"
        eType="#//Folder"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="Images"
        upperBound="-1"
        eType="#//Image"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="Name"
        lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Image">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="TagValues"
        upperBound="-1"
        eType="#//TagValue"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="Name"
        lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="TagValueCoordinates"
        upperBound="-1"
        eType="#//TagValueCoordinate"
        containment="true"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="TagCategory">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="Tags"
        upperBound="-1"
        eType="#//Tag"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="Name"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Tag">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="SubTag"
        upperBound="-1"
        eType="#//Tag"
        containment="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="Name"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="TagValue">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="Tag"
        lowerBound="1"
        eType="#//Tag"
        eKeys="#//Tag/Name"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="Value"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="SubTag"
        eType="#//Tag"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="SubValue"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="TagValueCoordinate">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="XPercentage"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="YPercentage"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="TagValue"
        eType="#//TagValue"/>
  </eClassifiers>
</ecore:EPackage>
