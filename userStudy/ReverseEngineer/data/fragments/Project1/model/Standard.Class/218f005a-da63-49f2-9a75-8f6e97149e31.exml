c.gridx = 0;
c.gridy = 0;
c.gridwidth = 3;
c.weightx = 1.0;  c.weighty = 0.0;
gb.setConstraints(persLabel, c);
configPanelNorth.add(persLabel);

JPanel persPanel = new JPanel(new BorderLayout());
JScrollPane persScroll =
