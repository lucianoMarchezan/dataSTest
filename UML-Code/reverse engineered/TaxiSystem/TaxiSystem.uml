          <ownedComment xmi:type="uml:Comment" xmi:id="_BQP7IrWpEeyVZOwzzImJqA">
            <body>if (taxiStruct.status == TaxiStruct.OFFLINE
    &amp;&amp; taxiStruct.alarm == false) {
    System.out.println("ComLinkC: confirmVoice failed!");
}
else {
    //voiceCom allowed
    if (voiceConnecting) {
        voiceAlive = true;
        voiceSend = true;
        voiceConnecting = false;
    }
    else
        System.out.println("ComLinkC: confirmVoice failed!");
}
</body>
