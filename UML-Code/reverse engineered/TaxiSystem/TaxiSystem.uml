          <ownedComment xmi:type="uml:Comment" xmi:id="_BQQB-7WpEeyVZOwzzImJqA">
            <body>boolean error = false;
if (taxiNumber &lt; 1) {
    Output.printError("Driver: taxiNumber out of range!");
    error = true;
}
if (driverNumber &lt; 1) {
    System.out.println("Driver: driverNumber out of range!");
    error = true;
}
if (!error) {
    System.out.println("new Driver()");
    this.driverNumber = driverNumber;
    this.taxiNumber = taxiNumber;
    taxiModule = new Taximodule(this);
}
else {
    this.driverNumber = -1;
    this.taxiNumber = -1;
    taxiModule = null;
}
</body>
