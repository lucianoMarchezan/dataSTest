      <eLiterals name="DBLCI" value="1"/>
      <eLiterals name="SLI" value="2"/>
      <eLiterals name="SBLCI" value="3"/>
      <eLiterals name="HPC" value="4"/>
      <eLiterals name="PMU" value="5"/>
      <eLiterals name="HTB" value="6"/>
